<?xml version="1.0" encoding="UTF-8"?>
<FacturaElectronica>

    <Clave> 4654564656546456421233518484787975646 </Clave>
    <CodigoActividad> 859456 </CodigoActividad>
    <NumeroConsecutivo> 001000010005000080009 </NumeroConsecutivo>
    <FechaEmision th:text="${facturas.getFecha()}" > </FechaEmision>
    <Emisor>

    <Nombre th:text="${proveedor.getNombre()}"></Nombre>
        <Identificacion>
            <Tipo> Tipo 1</Tipo>
            <Numero th:text="${proveedor.getCedula()}"></Numero>
        </Identificacion>

    </Emisor>

    <Receptor>
        <Nombre th:text="${clientes.getNombre()}"></Nombre>
        <Identificacion>
            <Tipo>Tipo 1</Tipo>
            <Numero th:text="${clientes.getCedula()}"></Numero>
        </Identificacion>
        <Correo th:text="${clientes.getCorreo()}"></Correo>
        <Telefono th:text="${clientes.getTelefono()}"></Telefono>
    </Receptor>

    <DetalleSerivicio>
        <th:block th:each="producto : ${productos}">
            <Producto>
                <Codigo th:text="${producto.getCodigo()}"></Codigo>
                <Nombre th:text="${producto.getNombre()}"></Nombre>
                <Precio th:text="${producto.getPrecio()}"></Precio>
            </Producto>
        </th:block>
    </DetalleSerivicio>

    <ResumenFactura>
        <CantidadProductos th:text="${facturas.getCantidad()}"></CantidadProductos>
        <MontoTotal th:text="${facturas.getMonto()}"></MontoTotal>
    </ResumenFactura>

</FacturaElectronica>